<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RuleML
	xmlns="http://svn.codehaus.org/logicabyss/RuleML2Drools/trunk/resources/rrml/pr.xsd">
	<Assert>
		<Rule>
			<if>
				<And>
					<And>
						<Atom>
							<op>
								<Rel>LessThan</Rel>
							</op>
							<Var>VAR302</Var>
							<Ind>13</Ind>
						</Atom>
						<Atom>
							<op>
								<Rel>Person</Rel>
							</op>
							<slot>
								<Ind>name</Ind>
								<Ind>john</Ind>
							</slot>
							<slot>
								<Ind>age</Ind>
								<Var>VAR302</Var>
							</slot>
						</Atom>
					</And>
					<Atom>
						<op>
							<Rel>Buy</Rel>
						</op>
						<slot>
							<Ind>buyer</Ind>
							<Var>$person</Var>
						</slot>
						<slot>
							<Ind>seller</Ind>
							<Var>$merchant</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Var>$object</Var>
						</slot>
					</Atom>
					<Atom>
						<op>
							<Rel>Keep</Rel>
						</op>
						<slot>
							<Ind>keeper</Ind>
							<Var>$person</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Ind>test</Ind>
						</slot>
					</Atom>
				</And>
			</if>
			<do>
				<Assert>
					<Atom>
						<op>
							<Rel>Own</Rel>
						</op>
						<slot>
							<Ind>owner</Ind>
							<Var>$person</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Ind>test</Ind>
						</slot>
					</Atom>
				</Assert>
			</do>
		</Rule>
	</Assert>
</RuleML>