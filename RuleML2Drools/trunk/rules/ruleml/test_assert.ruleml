<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RuleML xmlns="http://www.ruleml.org/1.0/xsd">
	<Assert>
		<Rule>
			<if>
				<And>
					<And>
						<Atom>
							<op>
								<Rel>LessThan</Rel>
							</op>
							<slot>
								<Ind>variable</Ind>
								<Var>VAR1</Var>
							</slot>
							<slot>
								<Ind>reference</Ind>
								<Ind>13</Ind>
							</slot>
						</Atom>
						<Atom>
							<op>
								<Rel>Person</Rel>
							</op>
							<slot>
								<Ind>name</Ind>
								<Ind>John</Ind>
							</slot>
							<slot>
								<Ind>age</Ind>
								<Var>VAR1</Var>
							</slot>
						</Atom>
					</And>
					<Atom>
						<op>
							<Rel>Buy</Rel>
						</op>
						<slot>
							<Ind>buyer</Ind>
							<Var>Person</Var>
						</slot>
						<slot>
							<Ind>seller</Ind>
							<Var>Merchant</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Ind>laptop</Ind>
						</slot>
					</Atom>
					<Atom>
						<op>
							<Rel>Keep</Rel>
						</op>
						<slot>
							<Ind>keeper</Ind>
							<Var>Person</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Ind>laptop</Ind>
						</slot>
					</Atom>
				</And>
			</if>
			<do>
				<Assert>
					<Atom>
						<op>
							<Rel>Own</Rel>
						</op>
						<slot>
							<Ind>owner</Ind>
							<Var>Person</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Ind>laptop</Ind>
						</slot>
					</Atom>
				</Assert>
			</do>
		</Rule>
		<Atom>
			<op>
				<Rel>Keep</Rel>
			</op>
			<slot>
				<Ind>keeper</Ind>
				<Ind>Ti6o</Ind>
			</slot>
			<slot>
				<Ind>item</Ind>
				<Ind>laptop</Ind>
			</slot>
		</Atom>
	</Assert>
</RuleML>