<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RuleML xmlns="http://www.ruleml.org/1.0/xsd">
    <Assert>
        <Rule>
            <if>
                <And>
                    <Atom>
                        <op>
                            <Rel>Person</Rel>
                        </op>
                        <oid>
                            <Var>$p</Var>
                        </oid>
                        <slot>
                            <Ind>name</Ind>
                            <Ind>Tisho</Ind>
                        </slot>
                        <slot>
                            <Ind>age</Ind>
                            <Var></Var>
                        </slot>
                    </Atom>
                    <Atom>
                        <op>
                            <Rel>Buy</Rel>
                        </op>
                        <oid>
                            <Var>$b</Var>
                        </oid>
                        <slot>
                            <Ind>buyer</Ind>
                            <Var>$p</Var>
                        </slot>
                        <slot>
                            <Ind>seller</Ind>
                            <Var>$s</Var>
                        </slot>
                        <slot>
                            <Ind>item</Ind>
                            <Ind>Object</Ind>
                        </slot>
                    </Atom>
                    <Atom>
                        <op>
                            <Rel>Keep</Rel>
                        </op>
                        <slot>
                            <Ind>keeper</Ind>
                            <Var>$p</Var>
                        </slot>
                        <slot>
                            <Ind>item</Ind>
                            <Ind>Object</Ind>
                        </slot>
                    </Atom>
                </And>
            </if>
            <do>
                <Retract>
                    <Atom>
                        <oid>
                            <Var>$b</Var>
                        </oid>
                    </Atom>
                </Retract>
            </do>
        </Rule>
    </Assert>
</RuleML>