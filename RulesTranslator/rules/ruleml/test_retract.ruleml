<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RuleML xmlns="http://www.ruleml.org/1.0/xsd">
	<Assert>
		<Rule>
			<if>
				<And>
					<Atom>
						<op>
							<Rel>Buy</Rel>
						</op>
						<slot>
							<Ind>buyer</Ind>
							<Ind>Ti6o</Ind>
						</slot>
						<slot>
							<Ind>seller</Ind>
							<Var>Seller</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Ind>ThinkPad</Ind>
						</slot>
					</Atom>
					<Atom>
						<op>
							<Rel>Person</Rel>
						</op>
						<slot>
							<Ind>name</Ind>
							<Ind>Ti6o</Ind>
						</slot>
						<slot>
							<Ind>age</Ind>
							<Var>Var1</Var>
						</slot>
					</Atom>
				</And>
			</if>
			<do>
				<Retract>
					<Atom>
						<op>
							<Rel>Buy</Rel>
						</op>
						<slot>
							<Ind>buyer</Ind>
							<Ind>Ti6o</Ind>
						</slot>
						<slot>
							<Ind>seller</Ind>
							<Var>Seller</Var>
						</slot>
						<slot>
							<Ind>item</Ind>
							<Ind>ThinkPad</Ind>
						</slot>
					</Atom>
				</Retract>
			</do>
		</Rule>
		<Atom>
			<op>
				<Rel>Own</Rel>
			</op>
			<slot>
				<Ind>owner</Ind>
				<Ind>Ti6o</Ind>
			</slot>
			<slot>
				<Ind>item</Ind>
				<Ind>laptop</Ind>
			</slot>
		</Atom>
	</Assert>
	<Retract>
		<Atom>
			<op>
				<Rel>Buy</Rel>
			</op>
			<slot>
				<Ind>buyer</Ind>
				<Ind>Ti6o</Ind>
			</slot>
			<slot>
				<Ind>seller</Ind>
				<Ind>Amazon</Ind>
			</slot>
			<slot>
				<Ind>item</Ind>
				<Ind>ThinkPad</Ind>
			</slot>
		</Atom>
	</Retract>
</RuleML>

